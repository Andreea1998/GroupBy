@model IEnumerable<Facebook.Models.Profile>

@{
    ViewBag.Title = "ShowFriends";
}
<ul>
    @foreach(var profile in Model)
    {
    <li>
		@Html.ActionLink(profile.name,"Show","Profile",new {@id=profile.userId },new {@class="strong" })
        <p>@profile.about</p>
        @if (ViewBag.loggedIn && (ViewBag.loggedInUser == ViewBag.ownsProfile))
        {
            @Html.ActionLink("Unfriend", "DeleteFriendRequest", "Friends", new { @requestBy = profile.userId, @requestTo = ViewBag.loggedInUser }, new { @class = "btn btn-success" })
        }
    </li>
    }
</ul>